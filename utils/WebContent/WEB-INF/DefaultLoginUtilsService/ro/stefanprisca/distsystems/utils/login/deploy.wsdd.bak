<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from DefaultLoginUtilsService WSDL service -->

  <service name="DefaultLoginUtils" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://login.utils.distsystems.stefanprisca.ro"/>
      <parameter name="wsdlServiceElement" value="DefaultLoginUtilsService"/>
      <parameter name="schemaQualified" value="http://login.utils.distsystems.stefanprisca.ro"/>
      <parameter name="wsdlServicePort" value="DefaultLoginUtils"/>
      <parameter name="className" value="ro.stefanprisca.distsystems.utils.login.DefaultLoginUtilsSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="DefaultLoginUtils"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getConnectConfirmation" qname="operNS:getConnectConfirmation" xmlns:operNS="http://login.utils.distsystems.stefanprisca.ro" returnQName="retNS:getConnectConfirmationReturn" xmlns:retNS="http://login.utils.distsystems.stefanprisca.ro" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="getConnectConfirmation"/>

  </service>
</deployment>
